package com.Hellowld;

import org.springframework.boot.CommandLineRunner;
import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;

@SpringBootApplication //to enable auto-configuration 
public class Jinnah2HellowldApplication implements CommandLineRunner //execute run method
{
    @Override
    public void run(String args[])
    {
        // Print statement when method is called
        System.out.println("Hello world");
    }
    
    
	public static void main(String[] args) {
		SpringApplication.run(Jinnah2HellowldApplication.class, args);
	}

}
